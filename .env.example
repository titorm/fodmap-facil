# Appwrite Configuration
EXPO_PUBLIC_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
EXPO_PUBLIC_APPWRITE_PROJECT_ID=your_project_id_here

# Database and Tables IDs (gerados após executar pnpm appwrite:setup)
EXPO_PUBLIC_APPWRITE_DATABASE_ID=your_database_id_here
EXPO_PUBLIC_APPWRITE_TABLE_TESTS_ID=your_tests_table_id_here
EXPO_PUBLIC_APPWRITE_TABLE_SYMPTOMS_ID=your_symptoms_table_id_here
EXPO_PUBLIC_APPWRITE_TABLE_PROTOCOL_RUNS_ID=your_protocol_runs_table_id_here
EXPO_PUBLIC_APPWRITE_TABLE_TEST_STEPS_ID=your_test_steps_table_id_here
EXPO_PUBLIC_APPWRITE_TABLE_SYMPTOM_ENTRIES_ID=your_symptom_entries_table_id_here
EXPO_PUBLIC_APPWRITE_TABLE_NOTIFICATION_HISTORY_ID=your_notification_history_table_id_here

# API Key para scripts de migração (apenas para desenvolvimento)
# Crie uma API Key no console do Appwrite com permissões de admin
APPWRITE_API_KEY=your_api_key_here
